
.frame-work
  width 100%
  height 100vh
  .logo
    height 54px
    width 54px
    border-radius 50%
    background #fff
    margin 18px auto
  .ant-menu
    margin 0
    padding 0
    .ant-menu-item
      margin 0
      padding 0
    .menu-link
      display flex
      align-items center
      .menu-icon
        font-size 22px

  .ant-layout-header
    display flex
    align-items center
    background #fff
    padding 0
    position sticky
    top 0
    right 0
    .toggle-icon
      font-size 32px
      margin-left 18px

  .ant-layout-content
    background #f0f0f0